<Window x:Class="WpfApplication1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1"
        mc:Ignorable="d"
        Title="MainWindow" Height="480" Width="320">
    <Grid>
        <Image x:Name="CalgaryLogo" HorizontalAlignment="Left" Height="120" Margin="26,28,0,0" VerticalAlignment="Top" Width="270" Source="chnw_186_large4presentations.jpg"/>
        <Button x:Name="buttonPointOfInterest" Content="Points of Interest" HorizontalAlignment="Left" Height="50" Margin="26,201,0,0" VerticalAlignment="Top" Width="106" Click="button_Click"/>
        <Button x:Name="buttonAboutCalgary" Content="About Calgary" HorizontalAlignment="Left" Height="50" Margin="181,201,0,0" VerticalAlignment="Top" Width="90" Click="buttonEvents_Click"/>
        <Button x:Name="buttonEvents" Content="Events" HorizontalAlignment="Left" Height="50" Margin="111,288,0,0" VerticalAlignment="Top" Width="90" Click="button_Copy1_Click"/>

    </Grid>
</Window>
